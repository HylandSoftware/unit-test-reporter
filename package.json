{
  "name": "unit-test-reporter",
  "version": "1.2.3",
  "private": true,
  "description": "Action to add nunit tests results ",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "clean": "ncc cache dir && ncc cache size && ncc cache clean",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test",
    "debug": "node --debug-brk --inspect ./node_modules/jest/bin/jest -i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HylandSoftware/unit-test-reporter.git"
  },
  "keywords": [
    "actions",
    "nunit",
    "test",
    "report"
  ],
  "author": "HylandSoftware",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@actions/glob": "^0.4.0",
    "@octokit/core": "^6.1.2",
    "@types/xml2js": "^0.4.14",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "@typescript-eslint/parser": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  }
}
