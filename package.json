{
  "name": "unit-test-reporter",
  "version": "1.2.3",
  "private": true,
  "description": "Action to add nunit tests results ",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "clean": "ncc cache dir && ncc cache size && ncc cache clean",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test",
    "debug": "node --debug-brk --inspect ./node_modules/jest/bin/jest -i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HylandSoftware/unit-test-reporter.git"
  },
  "keywords": [
    "actions",
    "nunit",
    "test",
    "report"
  ],
  "author": "HylandSoftware",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.0.3",
    "@actions/glob": "^0.3.0",
    "@octokit/core": "^4.0.4",
    "@types/xml2js": "^0.4.11",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.2",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.21.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^26.8.2",
    "jest": "^28.1.3",
    "jest-circus": "^28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
