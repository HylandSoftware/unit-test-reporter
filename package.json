{
  "name": "unit-test-reporter",
  "version": "1.1.1",
  "private": true,
  "description": "Action to add nunit tests results ",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "clean": "ncc cache dir && ncc cache size && ncc cache clean",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test",
    "debug": "node --debug-brk --inspect ./node_modules/jest/bin/jest -i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HylandSoftware/unit-test-reporter.git"
  },
  "keywords": [
    "actions",
    "nunit",
    "test",
    "report"
  ],
  "author": "HylandSoftware",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@actions/glob": "^0.1.1",
    "@octokit/core": "^3.3.1",
    "@types/xml2js": "^0.4.8",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.35",
    "@typescript-eslint/parser": "^4.20.0",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^7.25.0",
    "eslint-plugin-github": "^4.1.2",
    "eslint-plugin-jest": "^24.3.4",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
